{% extends 'base.html.twig' %}

{% block body %}
    <h3 class="teal-text lighten-4">Planilla de calificaciones</h3>
    

    <br>

      

    <ul>
        <li>

          <div class="col ">
                <a class="waves-effect waves-light btn" href={{ path('question_index') }}>Banco de Preguntas</a>
            </div>
            
        </li>
    </ul>

    {% for testTaken in testTaken %}
        <div class="col s12">
            <div class="col s10"><pre>{{ testTaken. }}</pre></div>
            <div class="input-field col s2">
                <p>
                    <input name="radio{{numQ}}"value="{{ans.idAnswerElement}}"type="radio" id="test{{ans.idAnswerElement}}"/>
                    <label class="teal-text lighten-4" for="test{{ans.idAnswerElement}}">Correcta</label>
                </p>
            </div>
        </div>
    {% endfor %}
{% endblock %}


