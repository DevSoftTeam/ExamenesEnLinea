
            {% set foo = 1 %}
            <div class="col s12">
              <div class="col s5">
              {% for i in range(0,((question['answers']|length) / 2)-1) %}
                <div class="col s1">
                  <label class="teal-text lighten-4"><h6>{{ foo }}. </h6></label>
                </div>
                <div class="col s11">
                  <pre>{{question['answers'][i].content}}</pre>
                </div>
              {% set foo = foo +1%}
              {% endfor %}
              </div>
              <div class="col s7">
              {% for k in range((question['answers']|length)/2, (question['answers']|length)-1) %}
              <div class="col s12">
                <div class="col s2">
                  <select name="match{{question['answers'][k].idAnswerElement}}">
                    <option value="" selected>Seleccione</option>
                    {% for j in range(1, (question['answers']|length)/2) %}
                    <option value="{{j}}" >{{ j }}</option>
                    {% endfor %}
                  </select>
                </div>
                <div class="col s10">
                  <pre>{{(question['answers'][k]).content}}</pre>
                </div>
              </div>
              {% endfor %}
              </div>
          </div>