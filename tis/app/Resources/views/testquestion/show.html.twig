{% extends 'base.html.twig' %}

{% block body %}
    <h1>Preguntas del examen</h1>

    <table>
        <tbody>
            <tr>
                <th>Porcentaje</th>
                <td>{{ testQuestion.percent }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('testquestion_index') }}">Volver a la lista</a>
        </li>
        <li>
            <a href="{{ path('testquestion_edit', { 'id': testQuestion.idQuestion }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Borrar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
